{
  "name": "cna-template-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "cross-env NODE_ENV=production next start",
    "generate": "graphql-codegen --config codegen.json",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "npm run lint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,md,css,scss,yaml,yml}\"",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "dependencies": {
    "@apollo/client": "3.3.4",
    "@apollo/react-hooks": "4.0.0",
    "@apollo/react-ssr": "4.0.0",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "cross-env": "7.0.3",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "next": "10.0.3",
    "next-compose-plugins": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "sass": "1.30.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@types/jest": "26.0.18",
    "@types/node": "14.14.11",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-jest": "26.6.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "graphql-codegen-apollo-next-ssr": "1.2.1",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "source-map-explorer": "2.5.1",
    "standard-version": "9.0.0",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run type-check && npm run format",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
